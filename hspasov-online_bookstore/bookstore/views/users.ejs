<!DOCTYPE html>
<html>
  <head>
    <meta charset="UTF-8">
    <title>Bookstore</title>
  </head>
  <body>
    <div id="content">
      <% if (users.length > 0) { %>
          <% users.forEach(function (user) { %>
            <p><%= user.id %></p>
            <p><%= user.username %></p>
            <p><%= user.email %></p>
            <p><%= user.first_name %></p>
            <p><%= user.last_name %></p>
            <p><%= user.country %></p>
            <p><%= user.address %></p>
            <p><%= user.phone_number %></p>
            <p><%= user.balance %></p>
            <p><%= user.currency %></p>
            <p><%= user.date_of_birth %></p>
            <p><%= user.created_at %></p>
            <p><%= user.updated_at %></p>
          <% });%>
        <% } else { %>
          <p>No results</p>
        <% }%>
    </div>
  </body>
</html>